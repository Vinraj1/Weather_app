{
  "version": 2,
  "builds": [
    {
      "src": "weather/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "weather/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "weather.settings"
  }
}
